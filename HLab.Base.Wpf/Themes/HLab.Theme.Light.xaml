<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Controls.xaml" />
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Fonts.xaml" />
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Themes/light.blue.xaml" />
    </ResourceDictionary.MergedDictionaries>
    
    <Color x:Key="HLab.Colors.Background">White</Color>
    <Color x:Key="HLab.Colors.Foreground">Black</Color>
    
    <Color x:Key="HLab.Colors.Header.Active.Background">#dee1e3</Color>
    <Color x:Key="HLab.Colors.Header.Active.Foreground">#87898d</Color>
    
    <Color x:Key="HLab.Colors.Header.Background">White</Color>
    <Color x:Key="HLab.Colors.Header.Foreground">#FF878274</Color>
    
    <Color x:Key="HLab.Colors.Selected.Background">#f3f3f3</Color>
    <Color x:Key="HLab.Colors.Selected.Foreground">#66686a</Color>
    
    <SolidColorBrush x:Key="HLab.Brushes.Background" Color="{StaticResource HLab.Colors.Background}"/>
    <SolidColorBrush x:Key="HLab.Brushes.Foreground" Color="{StaticResource HLab.Colors.Foreground}"/>
    
    <SolidColorBrush x:Key="HLab.Brushes.Header.Active.Background" Color="{StaticResource HLab.Colors.Header.Active.Background}"/>
    <SolidColorBrush x:Key="HLab.Brushes.Header.Active.Foreground" Color="{StaticResource HLab.Colors.Header.Active.Foreground}"/>

    <SolidColorBrush x:Key="HLab.Brushes.Header.Background" Color="{StaticResource HLab.Colors.Header.Background}"/>
    <SolidColorBrush x:Key="HLab.Brushes.Header.Foreground" Color="{StaticResource HLab.Colors.Header.Foreground}"/>
    
    <SolidColorBrush x:Key="HLab.Brushes.Selected.Background" Color="{StaticResource HLab.Colors.Selected.Background}"/>
    <SolidColorBrush x:Key="HLab.Brushes.Selected.Foreground" Color="{StaticResource HLab.Colors.Selected.Foreground}"/>
    
</ResourceDictionary>