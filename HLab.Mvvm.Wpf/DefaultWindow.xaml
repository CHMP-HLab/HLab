<Window 
    x:Class="HLab.Mvvm.DefaultWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:icons="clr-namespace:HLab.Mvvm.Icons"
    xmlns:lang="clr-namespace:HLab.Mvvm.Lang"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800"
    Title ="{Binding Title}"
    icons:IconView.IconService="{Binding IconService}"
    lang:Localize.LocalizationService="{Binding LocalizationService}"
    MouseDown="DefaultWindow_OnMouseDown"
>
</Window>
